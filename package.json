{
  "name": "blue-shot",
  "version": "0.0.1",
  "description": "Capture screenshots of elements for Protractor.",
  "keywords": [
    "protractor",
    "screenshot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/james-schwartzkopf/blue-shot"
  },
  "author": "James Schwartzkopf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/james-schwartzkopf/blue-shot/issues"
  },
  "homepage": "https://github.com/james-schwartzkopf/blue-shot",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build-watch": "tsc -p tsconfig.json --watch",
    "e2e": "protractor e2e/protractor.conf.js",
    "e2e-debug": "node --inspect-brk node_modules/protractor/bin/protractor e2e/protractor.conf.js",
    "e2e-travis": "protractor e2e/protractor-travis.conf.js",
    "tslint": "tslint -p tsconfig.json -p e2e/tsconfig.e2e.json",
    "e2e-update": "webdriver-manager update --gecko false",
    "serve-e2e-tests": "node e2e/e2e-serve.js"
  },
  "peerDependencies": {
    "pngjs": "^3.3.3",
    "selenium-webdriver": "^3.6.0"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.6",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "~8.9.4",
    "@types/pngjs": "^3.3.2",
    "finalhandler": "^1.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "mkdirp": "^0.5.1",
    "pngjs": "^3.3.3",
    "protractor": "~5.4.0",
    "serve-index": "^1.9.1",
    "serve-static": "^1.13.2",
    "ts-node": "~5.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "~5.9.1",
    "typescript": "~3.1.0"
  },
  "dependencies": {
    "uuencode": "^0.0.4"
  }
}
